########## UNSAFE, CHANGE CREDENTIALS AND FILENAME TO .env ##########

# SWAGGER
SWAGGER_PORT = 8080

# PGRST
PGRST_PORT = 3000
PGRST_USER = authenticator      # changes may also be done in in pgdbconf/schema.sql
PGRST_PASSWORD = pgrestauth     # changes may also be done in in pgdbconf/schema.sql
PGRST_OPENAPI_SERVER_PROXY_URI = http://localhost:3000 # change localhost to host-ip or hostname, when using a remot machine (without port, when e. g. nginx exposes this port)
PGRST_DB_SCHEMAS = api
PGRST_DB_ANON_ROLE = web_anon

# PGADM
PDADMIN_PORT = 5556
PGADMIN_DEFAULT_EMAIL = your@email.com
PGADMIN_DEFAULT_PW = rapsqladm

# PGDB
# POSTGRES_VERSION = 14.5
POSTGRES_PORT = 5432
POSTGRES_USER = your.username
POSTGRES_PASSWORD = rapsqladm
POSTGRES_DB = rapsql